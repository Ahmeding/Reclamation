<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.geek7aggart.gestion2reclamation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListeReclamationActivity"
            android:label="@string/title_activity_liste_reclamation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AjouterActivity"
            android:label="@string/title_activity_ajouter"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ModifierActivity"
            android:label="@string/title_activity_modifier"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ReclamtionActivity"
            android:label="@string/title_activity_reclamtion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".InterventionActivity"
            android:label="@string/title_activity_intervention"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>